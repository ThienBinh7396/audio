his.query.trim() == '') {\r\n                this.showToast({\r\n                    type: 'warning',\r\n                    text: 'Type something...'\r\n                })\r\n                return;\r\n            }\r\n            this.loading = true;\r\n\r\n            this.$axios.get('/story/search', {\r\n                    params: {\r\n                        query: this.query\r\n                    }\r\n                })\r\n                .then(rs => {\r\n                    let {\r\n                        data\r\n                    } = rs;\r\n\r\n                    this.stories = data.data;\r\n                    this.loading = false;\r\n                })\r\n        },\r\n        toStory: function (item) {\r\n\r\n            this.$router.push(`/story/${item.name}/${item.id}`);\r\n        },\r\n        closeSearchDialog: function(){\r\n            this.setShowSearch(false);\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/partials"}]}